{
  "compilerOptions": {
    "target":"es5",
    "lib":["es5","es6","dom"],
    "sourceMap":true,
    "module": "ES6",
    "jsx":"react",
    "experimentalDecorators": true,
    "removeComments":true,
    "preserveConstEnums": true,
    "moduleResolution":"node",
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "allowJs": false,
    "noImplicitAny": true
  },
  "files": [
    "core.ts",
    "sys.ts",
    "types.ts",
    "scanner.ts",
    "parser.ts",
    "utilities.ts",
    "binder.ts",
    "checker.ts",
    "emitter.ts",
    "program.ts",
    "commandLineParser.ts",
    "tsc.ts",
    "diagnosticInformationMap.generated.ts"
  ]
}